{
  "name": "operaciones",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/frontend/features/operaciones/src",
  "projectType": "library",
  "tags": ["type:feature", "scope:operaciones"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/frontend/features/operaciones",
        "main": "libs/frontend/features/operaciones/src/index.ts",
        "tsConfig": "libs/frontend/features/operaciones/tsconfig.build.json",
        "assets": []
      }
    },
    "lint": { "executor": "@nx/eslint:lint", "outputs": ["{options.outputFile}"] },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "config": "libs/frontend/features/operaciones/vitest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
