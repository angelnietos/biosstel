{
  "name": "api-users",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/backend/api-users/src",
  "projectType": "library",
  "tags": ["type:feature", "scope:api", "scope:api-users"],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/backend/api-users/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/backend/api-users",
        "main": "libs/backend/api-users/src/index.ts",
        "tsConfig": "libs/backend/api-users/tsconfig.json",
        "assets": ["libs/backend/api-users/*.md"]
      }
    }
  }
}
